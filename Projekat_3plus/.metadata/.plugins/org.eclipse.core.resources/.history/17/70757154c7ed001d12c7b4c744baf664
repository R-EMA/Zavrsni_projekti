package razvrstavanje;
/**
 * 
 * @author emma
 * 
 * zamena mesta elemenata niza
 * OK je samo za referentne tipove podataka
 * 
 * dodatak:
 * metod-i za štampanje članova niza
 *
 */
public class Swap {
	
	public static void swap(double x, double y) {
		double tmp;
		tmp = x;
		x = y;
		y = tmp;
	}
	
	public static void swap(String x, String y) {
		String tmp;
		tmp = x;
		x = y;
		y = tmp;
	}
	
	public static void swap(char x, char y) {
		char tmp;
		tmp = x;
		x = y;
		y = tmp;
	}
	
	
	
}
