package razvrstavanje;

/**
 * 
 * @author emma
 * 
 * različiti načini sortiranja nizova
 *
 */

public class Main {
	
	public static final void zamena(int x, int y) {
		int t;
		t = x;
		x = y;
		y = t;
	}
	
	public static void main(String[] args) {
		
		Integer[] niz = {6,3,0,5};
		int[] niz1 = {6,3,0,5};
		// sortirano (asc): {0.3.5.6}
		
		Swap.show_arr(niz);
		System.out.println("-----------");
		Sortiri.bubble_sort(niz);		
		Swap.show_arr(niz);
		
	}

}
